{
	"compilerOptions": {
		"esModuleInterop":true,
		"allowSyntheticDefaultImports":true,
		"baseUrl":".",
		"outDir":"build/dist",
		"target":"es5",
		"lib":["es6","dom"],
		"allowJs":true,
		"jsx":"react",
		"module": "commonjs",
		"moduleResolution":"node",
		"rootDir":"src",
		"noImplicitReturns":true,
		"noImplicitThis": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"strictNullChecks":true
	}
}